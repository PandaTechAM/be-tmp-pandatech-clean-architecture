{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Https": {
        "Url": "https://*:443",
        "Certificate": {
          "SslProtocols": [
            "Tls12",
            "Tls13"
          ],
          "Path": "/mnt/secrets/tls.crt",
          "KeyPath": "/mnt/secrets/tls.key"
        },
        "Protocols": "Http1AndHttp2AndHttp3"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  },
  "ResponseCrafterVisibility": "Private",
  "ConnectionStrings": {
    "Postgres": "**",
    "ElasticSearch": "**",
    "RabbitMQ": "**",
    "Redis": "**",
    "AuditTrail": "**"
  },
  "RabbitMQSettings": {
    "ExchangeName": "**",
    "RoutingKey": "**",
    "QueueName": "**",
    "RoutingKeyDLX": "**",
    "QueueNameDLX": "**"
  },
  "Security": {
    "SuperUser": {
      "Username": "**",
      "Password": "**"
    },
    "TokenExpirationMinutes": "**",
    "TokenMaxExpirationMinutes": "**",
    "AESKey": "**",
    "Base36Chars": "**"
  },
  "ElasticIndexName": "**"
}